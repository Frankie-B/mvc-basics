<div class="movie-container container-fluid">
  <div class="movieCard-container row">
    <div class="movie-card col-4">    
      <h1 class="movie-title">{{title}}</h1>
      <h4 class="movie-year">{{year}}</h4>
      <h3 class="movie-director">{{director}}</h3>
      <div class="movieLink-container">
        <a href="/movies/{{_id}}">go to detail page</a> 
        <a href="/movie/delete/{{_id}}">delete movie</a>
      </div> 
    </div>
  </div>
</div>
